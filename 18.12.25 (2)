#include <iostream>
using namespace std;

//Task 1

int main() {
    int price = 500;
    int quantity = 5;
    
    std::cout << "Итоговая стоимость: " << price * quantity << std::endl;
    std::cout << "Исходная цена: " << price << std::endl;
    std::cout << "Исходное количество: " << quantity << std::endl;
    
    return 0;
    
}

